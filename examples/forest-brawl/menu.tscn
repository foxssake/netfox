[sub_resource type="GDScript" id="GDScript_43xrg"]
script/source = "extends Button

@onready var target: Control = self.get_parent_control().get_child(0)
@onready var panel: Control = self.get_node(\"../..\")

func _pressed():
	if target.visible:
		target.hide()
		text = \">\"
	else:
		target.show()
		text = \"<\"
	panel.size_flags_horizontal ^= Control.SIZE_EXPAND
"

